<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hangzhou.tfchen.common.dao.IContentDao">
    <select id="findContent" parameterType="Integer" resultType="com.hangzhou.tfchen.common.bean.Content">
        SELECT id, name, type, content, img, url, createDate, modifyDate
        FROM common_content
        WHERE id = 1
    </select>
</mapper>
